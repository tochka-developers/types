{
    "name": "tochka-developers/types",
    "description": "Extended type system for PHP",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Babushkin Pavel",
            "email": "darkdarin@tochka.com"
        }
    ],
    "require": {
        "php": "8.2.*|8.3.*|8.4.*"
    },
    "require-dev": {
        "laravel/pint": "^1.17",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.0",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^5.26"
    },
    "autoload": {
        "psr-4": {
            "Tochka\\Types\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TochkaTest\\Types\\": "tests/"
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "psalm": "vendor/bin/psalm --output-format=phpstorm --no-cache"
    }
}
